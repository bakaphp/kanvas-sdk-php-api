{
  "require": {
    "php": ">= 7.1",
    "canvas/core": "dev-master",
    "bakaphp/canvas-sdk-php": "dev-master",
    "ext-openssl": "*",
    "ext-phalcon": ">=3.2.0 <4.0",
    "dariuszp/cli-progress-bar": "^1.0",
    "dmkit/phalcon-jwt-auth": "dev-master",
    "duncan3dc/fork-helper": "^2.2.0",
    "firebase/php-jwt": "^5.0",
    "lcobucci/jwt": "^3.2",
    "league/flysystem": "^1.0",
    "league/flysystem-aws-s3-v3": "^1.0",
    "league/fractal": "^0.17.0",
    "monolog/monolog": "^1.23",
    "namshi/notificator": " 1.0.*",
    "nesbot/carbon": "~1.18",
    "phalcon/incubator": "~3.4",
    "php-amqplib/php-amqplib": "^2.9",
    "psr/log": "^1.0",
    "pusher/pusher-php-server": "^3.0",
    "robmorgan/phinx": "^0.11",
    "sentry/sentry": "^1.9",
    "sly/notification-pusher": "2.x",
    "vlucas/phpdotenv": "^2.4",
    "elasticsearch/elasticsearch": "^6.1",
    "fzaninotto/faker": "^1.9@dev",
    "baka/cashier": "dev-master",
    "hybridauth/hybridauth": "dev-master"
  },
  "require-dev": {
    "codeception/codeception": "^2.4",
    "phalcon/ide-stubs": "^3.3",
    "phpstan/phpstan": "^0.9.2",
    "squizlabs/php_codesniffer": "^3.2",
    "symfony/browser-kit": "3.4",
    "symfony/console": "3.4",
    "symfony/css-selector": "3.4",
    "symfony/debug": "3.4",
    "symfony/dom-crawler": "3.4",
    "symfony/event-dispatcher": "3.4",
    "symfony/config": "3.4",
    "symfony/filesystem": "3.4",
    "symfony/finder": "3.4",
    "symfony/process": "3.4",
    "symfony/yaml": "3.4",
    "odan/phinx-migrations-generator": "dev-master",
    "mark-gerarts/auto-mapper-plus": "2.0.0-alpha1",
    "swiftmailer/swiftmailer": "^6",
    "stripe/stripe-php": "~6.0"
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  },
  "autoload": {
    "classmap": [
      "api",
      "library",
      "cli/tasks"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Niden\\Tests\\": "tests/",
      "Gewaer\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": "./vendor/bin/codecept run",
    "phpcs": "phpcs --standard=PSR2"
  }
}